&CONTROL
  calculation = 'relax'    
  forc_conv_thr =  5.0e-4 
  etot_conv_thr = 1.0e-5  
  max_seconds   =  1.72800e+05 
  nstep         = 100          
  pseudo_dir = '/home/ys/qepseudo/SSSP_1.3.0_PBE_efficiency' 
  outdir = './tmp'
  restart_mode = 'from_scratch' 
  prefix = 'ABN'  
  tprnfor = .true.        
  tstress = .true.       
  verbosity = 'high' 
/

&SYSTEM
    a           =  2.50000e+01
    ecutwfc = 30.0     
    ecutrho = 120.0   
    ibrav       = 1
    nat         = 15
    ntyp        = 3
    occupations = 'smearing'
    smearing = 'mv'     
    degauss = 0.02      
    nosym = .TRUE.
/

&ELECTRONS
    conv_thr         =  1.00000e-06
    electron_maxstep = 200
    mixing_beta      =  7.00000e-01
    startingpot      = "atomic"
    startingwfc      = "atomic+random"
/

&ELECTRONS
  conv_thr = 7.35e-7        
  electron_maxstep = 200
  mixing_beta = 0.7   
  diagonalization = 'david'  
  startingwfc = "atomic+random"
  startingpot = "atomic"
/

&IONS
    ion_dynamics = "bfgs"
/

&CELL
  cell_dofree = 'all'   
/

